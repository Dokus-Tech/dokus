  "queues": [
    {
      "name": "audit.events.queue",
      "vhost": "/dokus",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-dead-letter-exchange": "dokus.dlx",
        "x-dead-letter-routing-key": "dead"
