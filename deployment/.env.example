# Dokus Environment Configuration
# Copy this file to .env and fill in your values

# Database Configuration
DB_USERNAME=dokus
DB_PASSWORD=your-secure-database-password-here

# Redis Configuration
REDIS_PASSWORD=your-secure-redis-password-here

# RabbitMQ Configuration
RABBITMQ_USERNAME=dokus
RABBITMQ_PASSWORD=your-secure-rabbitmq-password-here

# JWT Configuration (Auth Service)
JWT_SECRET=your-super-secret-jwt-key-at-least-32-characters-long
JWT_ISSUER=https://yourdomain.com
JWT_AUDIENCE=dokus-api

# Logging
LOG_LEVEL=INFO

# Optional: Override default usernames
# DB_USERNAME=customuser
# RABBITMQ_USERNAME=customuser
