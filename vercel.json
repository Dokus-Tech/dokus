{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": null,
  "buildCommand": "./gradlew :composeApp:wasmJsBrowserDistribution -x test",
  "outputDirectory": "composeApp/build/dist/wasmJs/productionExecutable",
  "rewrites": [
    {
      "source": "/((?!.*\\.[^/]+$).*)",
      "destination": "/index.html"
    }
  ]
}
